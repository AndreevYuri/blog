{
  "name": "ngworkbox",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "npm-run-all --parallel sw-dev-webpack start",
    "start": "ng serve",
    "build": "ng build",
    "prebuild-prod": "shx rm -f ./src/service-worker.js",
    "build-prod": "ng build --prod",
    "postbuild-prod": "npm run sw-prod-webpack",
    "serve-dist": "ws --hostname localhost -d dist/ngworkbox -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "sw-dev-webpack": "shx rm -f ./src/service-worker.js && webpack --config ./sw-dev-webpack.config.js",
    "sw-prod-webpack": "shx rm -f ./dist/ngworkbox/service-worker.js && webpack --config ./sw-prod-webpack.config.js --progress --colors && workbox injectManifest ./workbox-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.7",
    "@angular/common": "9.0.7",
    "@angular/compiler": "9.0.7",
    "@angular/core": "9.0.7",
    "@angular/forms": "9.0.7",
    "@angular/platform-browser": "9.0.7",
    "@angular/platform-browser-dynamic": "9.0.7",
    "@angular/router": "9.0.7",
    "rxjs": "6.5.4",
    "tslib": "1.11.1",
    "workbox-core": "5.0.0",
    "workbox-precaching": "5.0.0",
    "workbox-window": "5.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.7",
    "@angular/cli": "9.0.7",
    "@angular/compiler-cli": "9.0.7",
    "@angular/language-service": "9.0.7",
    "@types/node": "13.9.2",
    "codelyzer": "5.2.1",
    "local-web-server": "4.0.0",
    "path": "0.12.7",
    "shx": "0.3.2",
    "npm-run-all": "4.1.5",
    "ts-loader": "6.2.1",
    "tslint": "6.1.0",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "workbox-cli": "5.0.0"
  }
}
