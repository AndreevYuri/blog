{
  "name": "syncclient",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "dev": "npm-run-all --parallel sw-dev-webpack ionic-start",
    "start": "ng serve -o",
    "ionic-start": "ionic serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "postbuild-prod": "npm run sw-prod-webpack",
    "serve-dist": "ws --hostname localhost -d dist/app -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "sw-dev-webpack": "rimraf ./src/service-worker.js && webpack --config ./sw-dev-webpack.config.js",
    "sw-prod-webpack": "rimraf ./dist/app/service-worker.js && webpack --config ./sw-prod-webpack.config.js --progress --colors && workbox injectManifest ./workbox-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "10.0.2",
    "@angular/compiler": "10.0.2",
    "@angular/core": "10.0.2",
    "@angular/forms": "10.0.2",
    "@angular/platform-browser": "10.0.2",
    "@angular/platform-browser-dynamic": "10.0.2",
    "@angular/router": "10.0.2",
    "@ionic/angular": "5.2.2",
    "dexie": "2.0.4",
    "rxjs": "6.5.5",
    "tslib": "2.0.0",
    "uuid": "8.2.0",
    "workbox-core": "5.1.3",
    "workbox-precaching": "5.1.3",
    "workbox-routing": "5.1.3",
    "workbox-strategies": "5.1.3",
    "workbox-window": "5.1.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.0",
    "@angular/cli": "10.0.0",
    "@angular/compiler-cli": "10.0.2",
    "@ionic/angular-toolkit": "2.2.0",
    "@types/node": "14.0.14",
    "codelyzer": "6.0.0-next.1",
    "local-web-server": "4.2.1",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-loader": "7.0.5",
    "tslint": "6.1.2",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "workbox-cli": "5.1.3"
  }
}
